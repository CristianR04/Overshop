@model IEnumerable<Inventariomodel>
@{

}

<head>
    <title></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <style>
        .fondo {
            background: url('/html/fondo.png');
            background-size: cover;
            background-position: center;
            height: 100vh;
            width: 100%;
            align-items: center;
            justify-content: center;
        }
    </style>


</head>
<body class="fondo">
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-info">
            <div class="container-fluid">
                <a class="navbar-brand" href="#">Overshop</a>
                <form asp-action="PDFi" asp-controller="PDFi">
                    <button type="submit" class="btn btn-secondary m-1">PDF Inventario</button>
                </form>
                <form asp-action="PDFr" asp-controller="PDFr">
                    <button type="submit" class="btn btn-secondary m-1">PDF Registro</button>
                </form>
                <form asp-action="PDF" asp-controller="PDF">
                    <button type="submit" class="btn btn-secondary m-1">PDF Proveedor</button>
                </form>
                <form asp-action="PDFc" asp-controller="PDFc">
                    <button type="submit" class="btn btn-secondary m-1">PDF Contacto</button>
                </form>
               
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    </ul>
                    <form class="d-flex m-1">
                        <input class="form-control input-group" type="search" placeholder="Buscar" aria-label="Search">
                        <button class="btn btn-outline-primary" type="submit"><img src="~/html/lupa.png" style="height: 1.3rem" /></button>
                    </form>
                    <form asp-action="Indexproductos" asp-controller="Home">
                        <div class="top-right-button">

                            <button type="submit" class="btn btn-outline-success">Productos</button>

                        </div>
                    </form>
                    <form asp-action="Proveedores" asp-controller="Proveedores">
                        <div class="top-right-button m-1">

                            <button type="submit" class="btn btn-outline-success">Proveedor</button>

                        </div>
                    </form>
                    <form asp-action="Compras" asp-controller="Compras">
                        <div class="top-right-button">

                            <button type="submit" class="btn btn-outline-success">Compras</button>

                        </div>
                    </form>
                </div>
            </div>
        </nav>
    </header>
    <div id="carouselExampleInterval" class="carousel slide" data-bs-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active" data-bs-interval="10000">
                <img src="/html/Overshop.png" class="mx-auto d-block w-50" alt="...">
            </div>
            <div class="carousel-item" data-bs-interval="2000">
                <img src="/html/Overshop.png" class="mx-auto d-block w-50" alt="...">
            </div>
            <div class="carousel-item">
                <img src="/html/Overshop.png" class="mx-auto d-block w-50" alt="...">
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Anterior</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleInterval" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Siguiente</span>
        </button>
    </div>
    <form asp-action="menu" asp-controller="Home">
        <div class="top-right-button m-1">

            <button type="submit" class="btn btn-danger">Cerrar Sesión</button>

        </div>
    </form>
    
   
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

</body>
